<script type="text/javascript">

    //* History.js
    function setHistoryHandler() {
        var History = window.History; // Note: We are using a capital H instead of a lower h

        if (!History.enabled) {
            return false;
        }

        History.Adapter.bind(window, ' statechange', function () {
            var State = History.getState();
            //History.log(State.data, State.title, State.url);
            var pageName = State.hash.replace("/auth/welcome/?pId=", "");
            comm.loadPage(pageName);
            return false;
        });
    }


    function init() {
        comm.setTitle();
        comm.setI18nMessage(DEFAULT_LANG);
        comm.applyI18n("container");
        var pageName = $("#pId").text();
        comm.loadPage(pageName);
        setHistoryHandler();
        setSideMenuHandler();
    }

    function setSideMenuHandler() {
        $(".side-menu li").click(function(){
            event.preventDefault();
            var menuName = $(this).attr('id').replace("menu-", "");
            History.pushState(null, null, "?pId=" + menuName);
        });
    }




    init();


</script>
